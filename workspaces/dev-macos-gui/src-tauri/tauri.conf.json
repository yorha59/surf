{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Surf",
  "version": "0.1.0",
  "identifier": "dev.surf",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Surf",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "identifier": "dev.surf",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "localhost",
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "signingIdentity": null
    }
  }
}
